@using Web_ALPA.Models.Entidad

@model List<BE_DevolucionCliente>

<html>
<body>
    <div>
        <table class="table table-striped- table-bordered table-hover table-checkable" id="kt_table_Scroller1x">
            <thead class="thead-dark">
                <tr role="row">
                    <th class="text-light">Fecha</th>
                    <th class="text-light">Código producto</th>
                    <th class="text-light">Código barra</th>
                    <th class="text-light">Descripción</th>
                    <th class="text-light">Cantidad</th>
                    <th class="text-light">Registrado por</th>
                    <th class="text-light">Fecha registro</th>
                </tr>
            </thead>
            <tbody>
                @if (Model != null)
                {
                    for (int i = 0; i < Model.Count; i++)
                    {
                        <tr style="text-align:justify">

                            <td class="text-dark">
                                @Html.HiddenFor(x => Model[i].Fecha)
                                @Html.DisplayFor(x => Model[i].Fecha)
                            </td>

                            <td class="text-dark">
                                @Html.HiddenFor(x => Model[i].U_CodArticulo)
                                @Html.DisplayFor(x => Model[i].U_CodArticulo)
                            </td>

                            <td class="text-dark">
                                @Html.HiddenFor(x => Model[i].U_CodBarras)
                                @Html.DisplayFor(x => Model[i].U_CodBarras)
                            </td>



                            <td class="text-dark">
                                @Html.HiddenFor(x => Model[i].U_DescArticulo)
                                @Html.DisplayFor(x => Model[i].U_DescArticulo)
                            </td>

                            <td class="text-dark">
                                @Html.HiddenFor(x => Model[i].U_Cantidad)
                                @Html.DisplayFor(x => Model[i].U_Cantidad)
                            </td>

                            <td class="text-dark">
                                @Html.HiddenFor(x => Model[i].U_UsuarioRegistro)
                                @Html.DisplayFor(x => Model[i].U_UsuarioRegistro)
                            </td>
                            <td class="text-dark">
                                @Html.HiddenFor(x => Model[i].U_FechaRegistro)
                                @Html.DisplayFor(x => Model[i].U_FechaRegistro)
                            </td>
                        </tr>
                    }
                }
            </tbody>
        </table>

    </div>
    <script src="~/assets/app/custom/general/components/datatables/MyTable/Table_Scrollable_V10.js"></script>
</body>
</html>




